{
  "name": "jelly",
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#0a0b12",
        "foreground": "#aeb9c4",
        "selectionForeground": "#ffffff",
        "findHighlight": "#ffe792",
        "findHighlightForeground": "#333333",
        "selectionBorder": "#ffffff",
        "bracketsForeground": "#aeb9c4",
        "bracketsOptions": "underline",
        "bracketContentsForeground": "#aeb9c4",
        "bracketContentsOptions": "underline",
        "tagsOptions": "stippled_underline"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#546b88"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#addbbc"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#61bcc6"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#6ba77f"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#6ba77f"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#228a96"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#6ba77f"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "",
        "foreground": "#228a96"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#6ba77f"
      }
