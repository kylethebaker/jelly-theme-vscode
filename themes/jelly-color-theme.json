        "foreground": "#8FBE00A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#64778a"
      }
    },
    {
      "scope": [
        "meta.property-value",
        "support.constant.property-value",
        "constant.other.color"
