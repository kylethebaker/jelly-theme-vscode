      "settings": {
        "foreground": "#e61f44"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A6E22E"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#f7b83d"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
