      "settings": {
        "fontStyle": "",
        "foreground": "#6ba77f"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "#e6eaef"
      }
    },
    {
      "name": "Function argument",
